<?xml version="1.0" encoding="utf-8"?>
<ScrollView  xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/white"
    tools:context=".ui.fragment.profile.ProfileFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <com.google.android.material.appbar.AppBarLayout
            android:id="@+id/appBarLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <com.google.android.material.appbar.MaterialToolbar
                android:id="@+id/topAppBar"
                android:contentDescription="Informasi Account"
                android:layout_width="match_parent"
                android:layout_height="?attr/actionBarSize"
                android:background="?attr/colorPrimary"
                app:title="Informasi Account"
                app:titleCentered="true"
                app:titleTextAppearance="@style/ToolbarTitleText" />
        </com.google.android.material.appbar.AppBarLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_margin="20dp">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:baselineAligned="false"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="3">
                    <ImageView
                        android:id="@+id/ivAccountProfile"
                        android:contentDescription="Foto Profil"
                        android:layout_gravity="top"
                        android:layout_width="70dp"
                        android:layout_height="100dp"
                        android:src="@drawable/account"/>
                </LinearLayout>
                <LinearLayout
                   android:layout_gravity="center"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">
                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/tvAccountName"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Nama Lenkap Anda"
                        android:contentDescription="Nama Lengkap Anda"
                        android:textSize="18sp"
                        android:fontFamily="@font/inter_bold"
                        android:textColor="@color/primary" />

                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/tvAccountEmail"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/inter"
                        android:contentDescription="Email Anda"
                        android:text="emailanda@gmail.com"
                        android:textColor="@color/black"
                        android:textSize="14sp" />

                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="3"
            android:orientation="vertical">
            <com.google.android.material.textview.MaterialTextView
                android:layout_marginBottom="5dp"
                android:layout_marginTop="20dp"
                android:id="@+id/tvAccountSettingTitle"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/pengaturan"
                android:contentDescription="Pengaturan"
                android:textColor="@color/black"
                android:textSize="18sp"
                android:textStyle="bold"
                app:fontFamily="@font/inter_bold" />
            <androidx.cardview.widget.CardView
                android:clickable="true"
                android:foreground="?android:attr/selectableItemBackground"
                android:contentDescription="Tombol ubah alamat email"
                android:layout_marginHorizontal="1dp"
                android:layout_marginVertical="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardElevation="2dp"
               app:cardCornerRadius="10dp">
                <LinearLayout

                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <com.google.android.material.textview.MaterialTextView
                        android:paddingVertical="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/black"
                        android:fontFamily="@font/inter_semibold"
                        android:text="@string/ubah_alamat_email"
                        android:paddingStart="10dp"
                        android:paddingEnd="10dp"
                        android:drawableStart="@drawable/ic_email"
                        android:drawableTint="@color/black"
                        android:textSize="14sp"
                        android:drawablePadding="10dp"
                        android:textAlignment="textStart"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        tools:ignore="RtlCompat" />
                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/tvAccountInformationMenu"
                        android:paddingVertical="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingLeft="10dp"
                        android:drawableEnd="@drawable/ic_next"
                        android:drawableTint="@color/black"
                        android:textSize="14sp"
                        android:layout_weight="7"
                        android:paddingRight="10dp"
                        android:gravity="center_horizontal|center_vertical" />
                </LinearLayout>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:clickable="true"
                android:foreground="?android:attr/selectableItemBackground"
                android:contentDescription="Tombol ubah password"
                android:layout_marginHorizontal="1dp"
                android:layout_marginVertical="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardElevation="2dp"
                app:cardCornerRadius="10dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <com.google.android.material.textview.MaterialTextView
                        android:paddingVertical="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/black"
                        android:fontFamily="@font/inter_semibold"
                        android:text="@string/ubah_password"
                        android:paddingStart="10dp"
                        android:paddingEnd="10dp"
                        android:drawableStart="@drawable/ic_lock"
                        android:drawableTint="@color/black"
                        android:textSize="14sp"
                        android:drawablePadding="10dp"
                        android:textAlignment="textStart"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        tools:ignore="RtlCompat" />
                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/tvAccountChangePasswordMenu"
                        android:paddingVertical="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingLeft="10dp"
                        android:drawableEnd="@drawable/ic_next"
                        android:drawableTint="@color/black"
                        android:textSize="14sp"
                        android:layout_weight="7"
                        android:paddingRight="10dp"
                        android:gravity="center_horizontal|center_vertical" />
                </LinearLayout>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:clickable="true"
                android:foreground="?android:attr/selectableItemBackground"
                android:contentDescription="Tombol ubah pin"
                android:layout_marginHorizontal="1dp"
                android:layout_marginVertical="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardElevation="2dp"
                app:cardCornerRadius="10dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <com.google.android.material.textview.MaterialTextView
                        android:paddingVertical="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/black"
                        android:fontFamily="@font/inter_semibold"
                        android:text="@string/ubah_pin"
                        android:paddingStart="10dp"
                        android:paddingEnd="10dp"
                        android:drawableStart="@drawable/ic_pin"
                        android:drawableTint="@color/black"
                        android:textSize="14sp"
                        android:drawablePadding="10dp"
                        android:textAlignment="textStart"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        tools:ignore="RtlCompat" />
                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/tvAccountChangePinMenu"
                        android:paddingVertical="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingLeft="10dp"
                        android:drawableEnd="@drawable/ic_next"
                        android:drawableTint="@color/black"
                        android:textSize="14sp"
                        android:layout_weight="7"
                        android:paddingRight="10dp"
                        android:gravity="center_horizontal|center_vertical" />
                </LinearLayout>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:clickable="true"
                android:foreground="?android:attr/selectableItemBackground"
                android:contentDescription="Tombol menu pintasan"
                android:layout_marginHorizontal="1dp"
                android:layout_marginVertical="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardElevation="2dp"
                app:cardCornerRadius="10dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <com.google.android.material.textview.MaterialTextView
                        android:paddingVertical="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/black"
                        android:fontFamily="@font/inter_semibold"
                        android:text="@string/menu_pintasan"
                        android:paddingLeft="12dp"
                        android:paddingRight="12dp"
                        android:drawableStart="@drawable/ic_four_squares"
                        android:drawableTint="@color/black"
                        android:textSize="14sp"
                        android:drawablePadding="12dp"
                        android:textAlignment="textStart"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        tools:ignore="RtlCompat" />
                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/tvAccountShortcutMenu"
                        android:paddingVertical="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingLeft="10dp"
                        android:drawableEnd="@drawable/ic_next"
                        android:drawableTint="@color/black"
                        android:textSize="14sp"
                        android:layout_weight="7"
                        android:paddingRight="10dp"
                        android:gravity="center_horizontal|center_vertical" />
                </LinearLayout>
            </androidx.cardview.widget.CardView>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="3"
            android:orientation="vertical">
            <com.google.android.material.textview.MaterialTextView
                android:layout_marginBottom="5dp"
                android:layout_marginTop="20dp"
                android:id="@+id/tvAccountHelpTitle"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/bantuan"
                android:contentDescription="Bantuan"
                android:textColor="@color/black"
                android:textSize="18sp"
                android:textStyle="bold"
                app:fontFamily="@font/inter_bold" />
            <androidx.cardview.widget.CardView
                android:clickable="true"
                android:foreground="?android:attr/selectableItemBackground"
                android:contentDescription="Tombol Live Chat"
                android:layout_marginHorizontal="1dp"
                android:layout_marginVertical="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardElevation="2dp"
                app:cardCornerRadius="10dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <com.google.android.material.textview.MaterialTextView
                        android:paddingVertical="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/black"
                        android:fontFamily="@font/inter_semibold"
                        android:text="@string/live_chat"
                        android:paddingStart="10dp"
                        android:paddingEnd="10dp"
                        android:drawableStart="@drawable/ic_live_chat"
                        android:drawableTint="@color/black"
                        android:textSize="14sp"
                        android:drawablePadding="10dp"
                        android:textAlignment="textStart"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        tools:ignore="RtlCompat" />
                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/tvAccountLiveChatMenu"
                        android:paddingVertical="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingLeft="10dp"
                        android:drawableEnd="@drawable/ic_next"
                        android:drawableTint="@color/black"
                        android:textSize="14sp"
                        android:layout_weight="7"
                        android:paddingRight="10dp"
                        android:gravity="center_horizontal|center_vertical" />
                </LinearLayout>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:clickable="true"
                android:foreground="?android:attr/selectableItemBackground"
                android:contentDescription="Tombol kontak kami"
                android:layout_marginHorizontal="1dp"
                android:layout_marginVertical="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardElevation="2dp"
                app:cardCornerRadius="10dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <com.google.android.material.textview.MaterialTextView
                        android:paddingVertical="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/black"
                        android:fontFamily="@font/inter_semibold"
                        android:text="@string/kontak_kami"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp"
                        android:drawableStart="@drawable/ic_contact_us"
                        android:drawableTint="@color/black"
                        android:textSize="14sp"
                        android:drawablePadding="10dp"
                        android:textAlignment="textStart"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        tools:ignore="RtlCompat" />
                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/tvAccountContactUsMenu"
                        android:paddingVertical="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingLeft="10dp"
                        android:drawableEnd="@drawable/ic_next"
                        android:drawableTint="@color/black"
                        android:textSize="14sp"
                        android:layout_weight="7"
                        android:paddingRight="10dp"
                        android:gravity="center_horizontal|center_vertical" />
                </LinearLayout>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:clickable="true"
                android:foreground="?android:attr/selectableItemBackground"
                android:contentDescription="Tombol syarat dan ketentuan"
                android:layout_marginHorizontal="1dp"
                android:layout_marginVertical="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardElevation="2dp"
                app:cardCornerRadius="10dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <com.google.android.material.textview.MaterialTextView
                        android:paddingVertical="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/black"
                        android:fontFamily="@font/inter_semibold"
                        android:text="@string/syarat_ketentuan"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp"
                        android:drawableStart="@drawable/ic_term_and_condition"
                        android:drawableTint="@color/black"
                        android:textSize="14sp"
                        android:drawablePadding="10dp"
                        android:textAlignment="textStart"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        tools:ignore="RtlCompat" />
                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/tvAccountTermAndConditionMenu"
                        android:paddingVertical="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingLeft="10dp"
                        android:drawableEnd="@drawable/ic_next"
                        android:drawableTint="@color/black"
                        android:textSize="14sp"
                        android:layout_weight="7"
                        android:paddingRight="10dp"
                        android:gravity="center_horizontal|center_vertical" />
                </LinearLayout>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:clickable="true"
                android:foreground="?android:attr/selectableItemBackground"
                android:contentDescription="Tombol kebijakan privasi"
                android:layout_marginHorizontal="1dp"
                android:layout_marginVertical="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardElevation="2dp"
                app:cardCornerRadius="10dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <com.google.android.material.textview.MaterialTextView
                        android:paddingVertical="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/black"
                        android:fontFamily="@font/inter_semibold"
                        android:text="@string/kebijakan_privasi"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp"
                        android:drawableStart="@drawable/ic_safety"
                        android:drawableTint="@color/black"
                        android:textSize="14sp"
                        android:drawablePadding="10dp"
                        android:textAlignment="textStart"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        tools:ignore="RtlCompat" />
                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/tvAccountPrivacyPolicyMenu"
                        android:paddingVertical="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingLeft="10dp"
                        android:drawableEnd="@drawable/ic_next"
                        android:drawableTint="@color/black"
                        android:textSize="14sp"
                        android:layout_weight="7"
                        android:paddingRight="10dp"
                        android:gravity="center_horizontal|center_vertical" />
                </LinearLayout>
            </androidx.cardview.widget.CardView>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="3"
            android:orientation="vertical">
            <com.google.android.material.textview.MaterialTextView
                android:layout_marginBottom="5dp"
                android:layout_marginTop="20dp"
                android:id="@+id/tvAccountInformationTitle"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/informasi"
                android:contentDescription="Informasi"
                android:textColor="@color/black"
                android:textSize="18sp"
                android:textStyle="bold"
                app:fontFamily="@font/inter_bold" />
            <androidx.cardview.widget.CardView
                android:clickable="true"
                android:foreground="?android:attr/selectableItemBackground"
                android:contentDescription="Tombol panduan pengguna"
                android:layout_marginHorizontal="1dp"
                android:layout_marginVertical="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardElevation="2dp"
                app:cardCornerRadius="10dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <com.google.android.material.textview.MaterialTextView
                        android:paddingVertical="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/black"
                        android:fontFamily="@font/inter_semibold"
                        android:text="@string/panduan_pengguna"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp"
                        android:drawableStart="@drawable/ic_developer_guide"
                        android:drawableTint="@color/black"
                        android:textSize="14sp"
                        android:drawablePadding="10dp"
                        android:textAlignment="textStart"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        tools:ignore="RtlCompat" />
                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/tvUserGuidanceMenu"
                        android:paddingVertical="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingLeft="10dp"
                        android:drawableEnd="@drawable/ic_next"
                        android:drawableTint="@color/black"
                        android:textSize="14sp"
                        android:layout_weight="7"
                        android:paddingRight="10dp"
                        android:gravity="center_horizontal|center_vertical" />
                </LinearLayout>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:clickable="true"
                android:foreground="?android:attr/selectableItemBackground"
                android:contentDescription="Tombol info kurs"
                android:layout_marginHorizontal="1dp"
                android:layout_marginVertical="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardElevation="2dp"
                app:cardCornerRadius="10dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <com.google.android.material.textview.MaterialTextView
                        android:paddingVertical="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/black"
                        android:fontFamily="@font/inter_semibold"
                        android:text="@string/info_kurs"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp"
                        android:drawableStart="@drawable/ic_currency"
                        android:drawableTint="@color/black"
                        android:textSize="14sp"
                        android:drawablePadding="10dp"
                        android:textAlignment="textStart"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        tools:ignore="RtlCompat" />
                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/tvAccountCurrencyInfo"
                        android:paddingVertical="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingLeft="10dp"
                        android:drawableEnd="@drawable/ic_next"
                        android:drawableTint="@color/black"
                        android:textSize="14sp"
                        android:layout_weight="7"
                        android:paddingRight="10dp"
                        android:gravity="center_horizontal|center_vertical" />
                </LinearLayout>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:clickable="true"
                android:foreground="?android:attr/selectableItemBackground"
                android:contentDescription="Tombol info saham"
                android:layout_marginHorizontal="1dp"
                android:layout_marginVertical="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardElevation="2dp"
                app:cardCornerRadius="10dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <com.google.android.material.textview.MaterialTextView
                        android:paddingVertical="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/black"
                        android:fontFamily="@font/inter_semibold"
                        android:text="@string/info_saham"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp"
                        android:drawableStart="@drawable/ic_chart"
                        android:drawableTint="@color/black"
                        android:textSize="14sp"
                        android:drawablePadding="10dp"
                        android:textAlignment="textStart"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        tools:ignore="RtlCompat" />
                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/tvMarketMenu"
                        android:paddingVertical="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingLeft="10dp"
                        android:drawableEnd="@drawable/ic_next"
                        android:drawableTint="@color/black"
                        android:textSize="14sp"
                        android:layout_weight="7"
                        android:paddingRight="10dp"
                        android:gravity="center_horizontal|center_vertical" />
                </LinearLayout>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:clickable="true"
                android:foreground="?android:attr/selectableItemBackground"
                android:contentDescription="Tombol lokasi ATM BCA"
                android:layout_marginHorizontal="1dp"
                android:layout_marginVertical="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardElevation="2dp"
                app:cardCornerRadius="10dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <com.google.android.material.textview.MaterialTextView
                        android:paddingVertical="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/black"
                        android:fontFamily="@font/inter_semibold"
                        android:text="@string/lokasi_atm_bca"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp"
                        android:drawableStart="@drawable/ic_money_exchange"
                        android:drawableTint="@color/black"
